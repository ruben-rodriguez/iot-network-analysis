              "region_name" : { "type" : "keyword" },
              "postal_code" : { "type" : "keyword" },
              "ip" : { "type" : "ip" },
              "location" : { "type" : "geo_point" },
              "latitude" : { "type" : "float" },
              "longitude" : { "type" : "float" },
              "timezone" : { "type" : "keyword" }
